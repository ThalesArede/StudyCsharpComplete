namespace HelloWorld {

class Main {

	public static void Main(String[] args) {

		Console.WriteLine("Digite a Hora inicial e final");

		int incialH, finalH, duracao;
		
		incialH = Console.ReadLine();
		finalH = Console.ReadLine();

		if (incialH < finalH) {
			duracao = finalH - incialH;
		} else {
			duracao = 24 - incialH + finalH;
		}

		Console.WriteLine("O JOGO DUROU " + duracao + " HORA(S)");

		Console.ReadLine();
	}
    }
}
